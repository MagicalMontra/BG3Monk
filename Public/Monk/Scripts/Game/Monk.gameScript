EVENT ApplyMartialArts
VARS
	CHARACTER:_Character
	FLOAT:_Dex
	FLOAT:_Str
	INT:_Result
ON
	OnItemEquipped(_Character, _Item)
	OnItemUnequipped(_Characer, _Item)
	OnTurn(_Character)
	OnCharacterVitalityChanged(_Character,_)
ACTIONS
	IF  "c1&c2&c3&(c4|c5)&!c6"
		 IsTagged(_Character, "MONK")
		CharacterGetStat(_Dex,_Character,Dexterity)
		CharacterGetStat(_Str,_Character,Strength)
		IsEqual(_Dex,_Str)
		IsGreaterThen(_Str,_Dex)
		CharacterHasStatus(_Character,"MARTS_STR")
	THEN
		CharacterApplyStatus(_Character, "MARTS_STR",-1)
		CharacterRemoveStatus(_Character,"MARTS_DEX")
		DisplayText(_Character,"MartialArts Strength")
	ELIF "c1&c2&c3&c4&!c5"
		IsTagged(_Character, "MONK")
		CharacterGetStat(_Dex,_Character,Dexterity)
		CharacterGetStat(_Str,_Character,Strength)
		IsGreaterThen(_Dex,_Str)
		CharacterHasStatus(_Character,"MARTS_DEX")
	THEN
		CharacterApplyStatus(_Character,"MARTS_DEX",-1)
		CharacterRemoveStatus(_Character,"MARTS_STR",-1)
		DisplayText(_Character,"MartialArts Dexterity")
	ENDIF

